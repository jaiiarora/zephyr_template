#The Kconfig file for System on Chip  series
#Select all the configuration options here

config SOC_SERIES_SM
	bool "Sm Series"
	select CPU_CORTEX_M
	select CPU_CORTEX_M0
	select SOC_FAMILY_SAMSUNG
	select SYS_POWER_LOW_POWER_STATE_SUPPORTED
	select CPU_HAS_SYSTICK
	select CLOCK_CONTROL
	help
	  Enable support for SM series
